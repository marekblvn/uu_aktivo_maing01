{
  "{asid}": {
    "profileList": ["AsidAuthorities", "AsidExecutives", "AsidLicenseOwner", "Public"],
    "useCaseMap": {}
  },
  "*": {
    "profileList": [
      "Authorities",
      "Executives",
      "AwidLicenseOwner",
      "Public",
      "Authenticated",
      "AwidInitiator",
      "StandardUsers"
    ],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created", "assigned"],
        "profileList": ["AwidLicenseOwner", "AwidInitiator"]
      },
      "sys/uuAppWorkspace/load": {
        "sysStateList": ["created", "assigned", "active", "restricted", "readOnly", "closed", "suspended"],
        "profileList": ["Authenticated"]
      },
      "sys/uuAppWorkspace/loadBasicData": {
        "sysStateList": ["created", "assigned", "active", "restricted", "readOnly", "closed", "suspended"],
        "profileList": ["Public"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created", "assigned"],
        "profileList": ["Public", "AwidLicenseOwner", "AwidInitiator"]
      },
      "activity/create": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/get": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/list": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/update": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/updateFrequency": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/updateNotificationOffset": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/addAdministrator": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      },
      "activity/removeAdministrator": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "StandardUsers"]
      }
    }
  }
}
